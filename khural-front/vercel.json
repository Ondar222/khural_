{
  "version": 2,
  "framework": "vite",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://someshit.yurta.site/:path*"
    },
    {
      "source": "/files/:path*",
      "destination": "https://someshit.yurta.site/files/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}






